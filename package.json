{
  "name": "vorpal",
  "version": "1.11.4",
  "description": "Node's first framework for building immersive CLI apps.",
  "main": "./dist/vorpal.js",
  "scripts": {
    "test": "gulp build; mocha;",
    "prepublish": "in-publish && gulp build || not-in-publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dthree/vorpal.git"
  },
  "keywords": [
    "api",
    "cli",
    "repl",
    "shell",
    "immersive",
    "framework",
    "app",
    "application",
    "command",
    "commander",
    "automated",
    "prompt",
    "inquirer"
  ],
  "author": "dthree",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dthree/vorpal/issues"
  },
  "homepage": "https://github.com/dthree/vorpal#readme",
  "devDependencies": {
    "babel": "6.23.0",
    "babel-core": "6.26.3",
    "babel-preset-es2015": "6.24.1",
    "bluebird": "3.7.2",
    "gulp": "4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-changed": "4.0.3",
    "gulp-eslint": "6.0.0",
    "gulp-xo": "0.25.0",
    "load-plugins": "2.1.2",
    "mocha": "10.2.0",
    "moment": "2.29.4",
    "request": "2.88.2",
    "should": "13.2.3",
    "vorpal-less": "0.0.13",
    "vorpal-repl": "1.1.8",
    "xo": "0.56.0"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "chalk": "5.3.0",
    "in-publish": "2.0.1",
    "inquirer": "9.2.11",
    "lodash": "4.17.21",
    "log-update": "5.0.1",
    "minimist": "1.2.8",
    "node-localstorage": "3.0.5",
    "strip-ansi": "7.1.0",
    "wrap-ansi": "8.1.0"
  },
  "engines": {
    "node": ">= 0.10.0",
    "iojs": ">= 1.0.0"
  },
  "xo": {
    "space": true,
    "rules": {
      "no-eval": 0,
      "no-unused-expressions": 0,
      "max-nested-callbacks": 0,
      "no-proto": 0,
      "wrap-iife": 0,
      "global-require": 0,
      "no-negated-condition": 0,
      "no-loop-func": 0,
      "no-implicit-coercion": 0,
      "no-use-extend-native/no-use-extend-native": 0,
      "no-undef": 0
    }
  },
  "files": [
    "dist"
  ]
}
